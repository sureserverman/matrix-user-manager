{
  "manifest_version": 2,
  "name": "Matrix User Manager",
  "version": "1.0",
  "description": "Manage Matrix/Synapse servers and provision users",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "Matrix User Manager"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}
